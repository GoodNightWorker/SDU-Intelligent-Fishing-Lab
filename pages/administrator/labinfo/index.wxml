<view class="title">实验室信息</view>
<view class="list">
    <view class="list-item" wx:for="{{listData}}" wx:for-item="listData_item">
        <view class="name">{{listData_item.name}}</view>
        <view class="value">{{listData_item.value}}</view>
    </view>
</view>
<view class="title">实验室设备</view>
<view class="device-list">
    <ai-devicecard wx:for="{{deviceList}}" wx:for-item="device_item" title="{{device_item.title}}" image="{{device_item.image}}" isOn="{{device_item.isOn}}"/>
</view>
